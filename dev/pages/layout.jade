block config
	include config

doctype html
html(lang="en")
	head
		title
			block title
				| #{PageTitle}

		meta(name="keywords" content=Keywords )
		meta(name="description" content=description )

		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		//- GOOGLE fonts
		if googleFonts
			link( href="http://fonts.googleapis.com/css?family=" + googleFonts, rel="stylesheet", type="text/css")

		//- jQuery (necessary for Bootstrap's JavaScript plugins)
		if jQuery
			script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")

		//- bootstrap gallery CSS
		//-if bootstrapGallery
			link(rel="stylesheet" href="#{assets}/css/bootstrap_image_gallery/blueimp-gallery.css")
			//link(rel="stylesheet" href="#{assets}/css/bootstrap_image_gallery/bootstrap-image-gallery.min.css")



		//- Bootstrap
		link(href="#{assets}/css/style.css" rel="stylesheet")

		// HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
		// WARNING: Respond.js doesn't work if you view the page via file://
		// [if lt IE 9]><script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script><script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]


		//- Mordernizr
		if Mordernizr
			script(src="#{assets}/js/modernizr-2.6.2.min.js")

		//- bootstrapGalleryV02
		if bootstrapGalleryV02
			//- link(rel="stylesheet" href="#{assets}/css/jquery-flickr-photoset-master/bootstrap-3.1.1/css/bootstrap.min.css")
			link(rel="stylesheet" href="#{assets}/css/jquery-flickr-photoset-master/blueimp-gallery.css")
			link(rel="stylesheet" href="#{assets}/css/jquery-flickr-photoset-master/bootstrap-image-gallery.css")
			link(rel="stylesheet" href="#{assets}/css/jquery-flickr-photoset-master/site.css")
			//-script(src="#{assets}/js/jquery-flickr-photoset-master/jquery-1.11.1.min.js")
			//-script(src="#{assets}/js/jquery-flickr-photoset-master/bootstrap-3.1.1/js/bootstrap.min.js")
			script(src="#{assets}/js/jquery-flickr-photoset-master/imagesloaded.pkgd.min.js")
			script(src="#{assets}/js/jquery-flickr-photoset-master/jquery.blueimp-gallery.min.js")
			script(src="#{assets}/js/jquery-flickr-photoset-master/flickr-jquery.js")
			//-script(src="#{assets}/js/jquery-flickr-photoset-master/Bootstrap-Image-Gallery-3.1.0/js/bootstrap-image-gallery.min.js")

	//- body class for any page
	body(class="#{bodyClass}")
		//- h1 #{site}
		//- block menu
			//- Menu
			include menu.jade

		block content


		//- insertscript
		block script
			//-bootstrap
			if bootstrap
				script(src="#{assets}/js/bootstrap.min.js")

			//-paralax
			if paralax
				script(src="#{assets}/js/paralax.js")

			//-maps
			//-if gmaps
				script(src="http://maps.google.com/maps/api/js?sensor=false")
				//-script(src="#{assets}/js/gmaps.js")
				script(src="#{assets}/js/mymap.js")

			//-flexslider
			//-if flexslider
				script(src="#{assets}/js/flexSlider/jquery.flexslider-min.js")
				script(src="#{assets}/js/flexSlider/flexslider.js")

			//-JsFlickrGallery
			//-if JsFlickrGallery
				script(type="text/javascript" src="#{assets}/js/JsFlickrGallery/js-flickr-gallery.js")
				link(rel="stylesheet" type="text/css" href="#{assets}/css/JsFlickrGallery/js-flickr-gallery.css")


			//-if bootstrapGallery
				//- we have already JQuerry
				//- script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
				script(src="#{assets}/js/bootstrap_image_gallery/jquery.blueimp-gallery.min.js")
				script(src="#{assets}/js/bootstrap_image_gallery/blueimp-gallery-fullscreen.js")
				script


				//-script(src="#{assets}/js/bootstrap_image_gallery/bootstrap-image-gallery.js")
				script(src="#{assets}/js/bootstrap_image_gallery/demo.js")
